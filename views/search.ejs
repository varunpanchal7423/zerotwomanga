<script>
  window.onload =() =>{
  const jsonStr = <%- locals.data %>;
  for(i=0;i<jsonStr.length;i++){
    
    const div_card = document.createElement("div");
    div_card.classList.add("card");
    
    const mangaurl = "/manga/"+jsonStr[i].id
    const a = document.createElement("a");
    a.classList.add("tag-a")
    a.setAttribute("href",mangaurl)

    const div_card_image = document.createElement("div");
    div_card_image.classList.add("card_image");

    const img = document.createElement("img");
    img.id = i

    for(let j =0;j<jsonStr[i].relationships.length;j++){
      if(jsonStr[i].relationships[j].type == "cover_art"){
        const url = "/data?imgurl="+ "https://uploads.mangadex.org/covers/"+jsonStr[i].id+"/"+jsonStr[i].relationships[j].attributes.fileName;
        const xhr = new XMLHttpRequest();
        xhr.open('GET', url);
        xhr.send();
        xhr.onload = function() {
        const arrayBuffer = xhr.response
        img.setAttribute("src","data:image/jpeg;base64,"+arrayBuffer)
        };
      }
    }

    const div_card_title = document.createElement("div");
    div_card_title.classList.add("card_title", "title-black")

    const p = document.createElement("p");

    const text = document.createTextNode(jsonStr[i].attributes.title.en);

    p.appendChild(text);
    div_card_title.appendChild(p);
    div_card_image.appendChild(img);
    div_card.appendChild(div_card_image);
    div_card.appendChild(div_card_title)
    a.appendChild(div_card)

    const main_card = document.getElementById('cards-list')
    main_card.append(a)

    
  }
  }
</script>

